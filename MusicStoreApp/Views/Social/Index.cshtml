      <script type="text/html" id="display">
          <td><img data-bind="attr:{ src:imgUrl }" src="../" style="height:100px;width:100px;" /></td>
        <td data-bind="text:Name" class="span3"></td>
        <td data-bind="text:Category" class="span3"></td>
        <td data-bind="text:Price" class="span3"></td>
        <td class="span3">
            <button class="btn btn-success kout-edit" type="button">Edit</button>
            <button class="btn btn-danger kout-delete" type="button">Delete</button>
        </td>

    </script>

    <script type="text/html" id="edit">
        <td><img src="../" data-bind="attr:{ src:imgUrl }" style="height:100px;width:100px;" /></td>
        <td class="span3"><input type="text" data-bind="value:Name" class="input-small" /></td>
        <td class="span3"><input type="text" data-bind="value:Category" class="input-small" /></td>
        <td class="span3"><input type="text" data-bind="value:Price" class="input-small" /></td>
        <td class="span3">
            <button class="btn btn-success kout-update" type="button">Update</button>
            <button class="btn btn-danger kout-cancel" type="button">Cancel</button>
        </td>
    </script>
   